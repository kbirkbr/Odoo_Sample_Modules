<odoo>
	
	<record id="watch_video_form" model="ir.ui.view">
		<field name="name">watch.video.form</field>
		<field name="model">watch.video</field>
		 <field name="arch" type="xml">
		 	<form string="Watch Video" edit="false">
        		<field name="url" widget="html" options="{'style-inline': false}" nolabel="1" readonly="1"/>
		 	 <footer>
		 	 	<button string="Close" class="btn-default" special="cancel"/> 
		 	 </footer>
		 	</form>
		 	
		 </field>
	</record>

</odoo>